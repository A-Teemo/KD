<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>知晓</title>
    <%- include head %>
    <link rel="stylesheet" href="/stylesheets/index.css">
    <script src="https://cdn.bootcss.com/moment.js/2.17.1/moment.min.js"></script>
    <script src="/javascripts/index.js" charset="utf-8"></script>

  </head>
  <body>
    <%- include nav %>
    <div class="con">
        <div class="container">
          <div class="row col-md-10 col-md-offset-1">
            <div class="right col-sm-9">
                <% data.forEach(function(ele,index){ %>
                  <div class="row" style="margin:10px auto;background-color:#fff;padding:10px 20px;border-radius:10px;box-shadow:0 0 15px">
                    <p><h4><a href="/show-xq?id=<%= ele._id %>" style="font-size:20px;">question：<%= ele.user_quiz %></a></h4></p>
                  <span class="btn btn-warning btn-xs">author:<%= ele.user_id %></span>&nbsp;&nbsp;
                  <span class="btn btn-info btn-xs">mark:<%= ele.user_label %></span>&nbsp;&nbsp;
                  <span class="btn btn-inverse btn-xs">time:<%= moment(ele.user_time).format('YYYY-MM-DD h:mm:ss a') %></span>
                  </div>
                <% }) %>

            </div>
            <div class="left col-sm-3 hidden-xs">
              <div class="left_all">
                  asdasd
              </div>
            </div>
          </div>
        </div>
    </div>

    <%- include footer %>



  </body>
</html>
